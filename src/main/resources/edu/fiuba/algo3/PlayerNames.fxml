<?import javafx.scene.layout.StackPane?>

<StackPane xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <ImageView fitHeight="650.0" fitWidth="650.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../../images/UserInformationScreen.png" />
         </image>
      </ImageView>
      <Button id="button" mnemonicParsing="false" onAction="" prefHeight="40.0" prefWidth="180.0" stylesheets="@css/textFormat.css" text="Continuar" StackPane.alignment="BOTTOM_CENTER">
